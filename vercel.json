{
  "version": 2,
  "builds": [
    {
      "src": "apps/frontend/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "apps/backend/api/**/*.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}